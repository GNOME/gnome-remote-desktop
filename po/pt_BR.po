#: data/org.gnome.remotedesktop.configure-system-daemon.policy.in:11
msgid "Allow to configure gnome-remote-desktop's system daemon settings"
msgstr ""
"Permitir definir as configurações do daemon do sistema do gnome-remote-"
"desktop"

#: data/org.gnome.remotedesktop.configure-system-daemon.policy.in:12
msgid ""
"Authentication is required to configure gnome-remote-desktop's system daemon "
"settings"
msgstr ""
"A autenticação é necessária para definir as configurações do daemon do "
"sistema do gnome-remote-desktop"

#: data/org.gnome.remotedesktop.enable-system-daemon.policy.in:11
msgid "Allow to enable and disable gnome-remote-desktop's system daemon"
msgstr ""
"Permitir ativar e desativar o daemon do sistema do gnome-remote-desktop"

#: data/org.gnome.remotedesktop.enable-system-daemon.policy.in:12
msgid ""
"Authentication is required to enable or disable gnome-remote-desktop's "
"system daemon"
msgstr ""
"A autenticação é necessária para ativar ou desativar o daemon do sistema do "
"gnome-remote-desktop"

#: src/grd-ctl.c:72
#, c-format
msgid "Usage: %s [OPTIONS...] COMMAND [SUBCOMMAND]...\n"
msgstr "Uso: %s [OPÇÕES...] COMANDO [SUBCOMANDO]...\n"

#: src/grd-ctl.c:382
msgid "Username: "
msgstr "Nome de usuário: "

#: src/grd-ctl.c:394 src/grd-ctl.c:552
msgid "Password: "
msgstr "Senha: "

#: src/grd-ctl.c:682
msgid "Commands:\n"
msgstr "Comandos:\n"

#: src/grd-ctl.c:687
msgid ""
"  rdp                                            - RDP subcommands:\n"
"    set-port                                     - Set port the server binds "
"to\n"
"    enable                                       - Enable the RDP backend\n"
"    disable                                      - Disable the RDP backend\n"
"    set-auth-methods <list-of-methods>           - Set a comma separated "
"list of\n"
"                                                   allowed authentication "
"methods\n"
"                                                   (credentials, kerberos)\n"
"    set-tls-cert <path-to-cert>                  - Set path to TLS "
"certificate\n"
"    set-tls-key <path-to-key>                    - Set path to TLS key\n"
"    set-kerberos-keytab <path-to-keytab>         - Set path to a Kerberos "
"keytab\n"
"    set-credentials [<username> [<password>]]    - Set username and "
"password\n"
"                                                   credentials\n"
"    clear-credentials                            - Clear username and "
"password\n"
"                                                   credentials\n"
"    enable-view-only                             - Disable remote control of "
"input\n"
"                                                   devices\n"
"    disable-view-only                            - Enable remote control of "
"input\n"
"                                                   devices\n"
"    enable-port-negotiation                      - If unavailable, listen "
"to\n"
"                                                   a different port\n"
"    disable-port-negotiation                     - If unavailable, don't "
"listen\n"
"                                                   to a different port\n"
"\n"
msgstr ""
"  rdp                                        - Subcomandos RDP:\n"
"    set-port                                 - Define a porta à qual o "
"servidor\n"
"                                               se liga\n"
"    enable                                   - Habilita o backend RDP\n"
"    disable                                  - Desabilita o backend RDP\n"
"    set-auth-methods <lista-de-métodos>      - Define uma lista separada \n"
"                                               por vírgulas de métodos de \n"
"                                               autenticação permitidos\n"
"                                               (credenciais, kerberos)\n"
"    set-tls-cert <caminho-do-cert>           - Define caminho para "
"certificado\n"
"                                               TLS\n"
"    set-tls-key <caminho-do-cert>            - Define caminho para chave "
"TLS\n"
"    set-kerberos-keytab <caminho-do-keytab>  - Define caminho para chave \n"
"                                               privada Kerberos \n"
"    set-credentials <usuário> <senha>        - Define credenciais de nome "
"de\n"
"                                               usuário e senha\n"
"    clear-credentials                        - Limpa credenciais de nome de\n"
"                                               usuário e senha\n"
"    enable-view-only                         - Desabilita o controle remoto "
"de\n"
"                                               dispositivos de entrada\n"
"    disable-view-only                        - Habilita o controle remoto "
"de\n"
"                                               dispositivos de entrada\n"
"    enable-port-negotiation                  - Se não disponível, escuta "
"uma\n"
"                                               porta diferente\n"
"    disable-port-negotiation                 - Se não disponível, não "
"escuta\n"
"                                               uma porta diferente\n"
"\n"

#: src/grd-ctl.c:715
msgid ""
"  vnc                                        - VNC subcommands:\n"
"    set-port                                 - Set port the server binds to\n"
"    enable                                   - Enable the VNC backend\n"
"    disable                                  - Disable the VNC backend\n"
"    set-password [<password>]                - Set the VNC password\n"
"    clear-password                           - Clear the VNC password\n"
"    set-auth-method password|prompt          - Set the authorization method\n"
"    enable-view-only                         - Disable remote control of "
"input\n"
"                                               devices\n"
"    disable-view-only                        - Enable remote control of "
"input\n"
"                                               devices\n"
"    enable-port-negotiation                  - If unavailable, listen to\n"
"                                               a different port\n"
"    disable-port-negotiation                 - If unavailable, don't listen\n"
"                                               to a different port\n"
"\n"
msgstr ""
"  vnc                                        - Subcomandos VNC:\n"
"    set-port                                 - Define a porta à qual o "
"servidor\n"
"                                               se liga\n"
"    enable                                   - Habilita o backend VNC\n"
"    disable                                  - Desabilita o backend VNC\n"
"    set-password <senha>                     - Define a senha do VNC\n"
"    clear-password                           - Limpa a senha do VNC\n"
"    set-auth-method password|prompt          - Define o método de "
"autorização\n"
"    enable-view-only                         - Desabilita o controle remoto "
"de\n"
"                                               dispositivos de entrada\n"
"    disable-view-only                        - Habilita o controle remoto "
"de\n"
"                                               dispositivos de entrada\n"
"    enable-port-negotiation                  - Se não disponível, escuta "
"uma\n"
"                                               porta diferente\n"
"    disable-port-negotiation                 - Se não disponível, não "
"escuta\n"
"                                               uma porta diferente\n"
"\n"

